{% extends '::base.html.twig' %}

{% block content %}
    <table class="table-bordered">
        <th>#</th><th>Name project</th>
        {% for item in data %}
            {% for project in item %}
                <tr>
                    <td width="20px">{{ loop.index }}</td>
                    <td><a href="{{ path('show_project', {'id': project.id}) }}">{{ project.name }}</a></td>
                </tr>
            {% endfor %}
        {% endfor %}
    </table>
{% endblock %}